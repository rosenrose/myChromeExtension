{
    "manifest_version": 3,
    "name": "My Extensions",
    "version": "1.0",
    "description" : "Base Level Extension",
    "action": {
        "default_popup": "options.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab",
        "tabs",
        "webNavigation",
        "contextMenus"
    ],
    "options_page": "options.html",
    "content_scripts": [{
        "matches": [
            "*://*/*"
        ],
        "exclude_matches": [],
        "js": [
            "script.js"
        ],
        "css":[
            "style.css"
        ]
    }]
}